<project name="Sample usage of Salesforce Ant tasks" default="test" basedir="." xmlns:sf="antlib:com.salesforce">

  <property file="build.properties"/>
  
  <property environment="env"/>
  
  <!-- Deploy the unpackaged set of metadata retrieved with retrieveUnpackaged and run tests in this organizationâ€™s namespace only -->
  <taskdef resource="com/salesforce/antlib.xml" uri="antlib:com.salesforce">
    <classpath>
      <pathelement location="../ant-salesforce.jar"/>
    </classpath>
  </taskdef>
  <target name="deployall">
  
  <sf:deploy username="${sf.username}" password="${sf.password}" serverurl="${sf.serverurl}" testlevel="NoTestRun" logtype="Detail" deployRoot="force-app/main/default"/>
  
  </target>
  
  </project>